alias gl='git log --graph --pretty=format:%C(magenta)%h%Creset -%C(red)%d%Creset %s %C(dim green)(%cr) %C(cyan)<%an>%Creset --abbrev-commit'

gacp(){
gaa
gc -m "$1"
gp
}

